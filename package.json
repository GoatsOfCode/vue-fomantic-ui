{
  "name": "vue-fomantic-ui",
  "version": "0.11.0",
  "description": "Fomantic-UI component library for Vue.js 3.0",
  "license": "MIT",
  "bugs": "https://github.com/nightswinger/vue-fomantic-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nightswinger/vue-fomantic-ui"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-fomantic-ui.umd.js",
  "module": "./dist/vue-fomantic-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-fomantic-ui.es.js",
      "require": "./dist/vue-fomantic-ui.umd.js"
    }
  },
  "keywords": [
    "semantic-ui",
    "fomantic-ui",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.1",
    "@vitejs/plugin-vue-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "typescript": "^4.1.3",
    "vite": "^2.7.13",
    "vue": "^3.0.5"
  }
}
